<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scheduledWorkoutsFragmentItem"
    android:layout_width="match_parent"
    android:layout_height="64dp"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="@dimen/material_list_two_lines_side_padding"
    android:paddingRight="@dimen/material_list_two_lines_side_padding">

    <TextView
        android:id="@+id/createdWorkoutsHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_regular"
        android:textSize="@dimen/materialListHeaderSize"
        android:text="aa sger igejr "
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="12dp"/>
    <TextView
        android:id="@+id/createdWorkoutsSubHeader"
        android:layout_width="wrap_content"
        android:text="fbhwereg iugegte"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_regular"
        app:layout_constraintTop_toBottomOf="@+id/createdWorkoutsHeader"
        android:layout_marginTop="6dp"
        android:textSize="@dimen/materialListSubHeaderSize"
        />

    <ImageView
        android:id="@+id/btnOpenScheduleWorkout"
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:srcCompat="@drawable/ic_baseline_add_24px"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</android.support.constraint.ConstraintLayout>