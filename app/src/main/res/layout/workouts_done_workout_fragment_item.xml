<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scheduledWorkoutsFragmentItem"
    android:layout_width="match_parent"
    android:layout_height="64dp"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="@dimen/material_list_two_lines_side_padding"
    android:paddingRight="@dimen/material_list_two_lines_side_padding">

    <TextView
        android:id="@+id/doneWorkoutsHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_regular"
        android:textSize="@dimen/materialListHeaderSize"
        android:textColor="@color/primary_text"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Morning yoga"
        android:layout_marginTop="12dp"/>
    <TextView
        android:id="@+id/doneWorkoutsSubHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_regular"
        app:layout_constraintTop_toBottomOf="@+id/doneWorkoutsHeader"
        android:layout_marginTop="6dp"
        android:textSize="@dimen/materialListSubHeaderSize"
        android:text="15/12"
        />
    <TextView
        android:id="@+id/doneWorkoutsSubHeader2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_regular"
        app:layout_constraintTop_toBottomOf="@+id/doneWorkoutsHeader"
        app:layout_constraintLeft_toRightOf="@+id/doneWorkoutsSubHeader"
        android:layout_marginTop="6dp"
        android:textSize="@dimen/materialListSubHeaderSize"
        android:text="13:30"
        android:layout_marginLeft="10dp"
        />

